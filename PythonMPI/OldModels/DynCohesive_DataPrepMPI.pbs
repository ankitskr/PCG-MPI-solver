source ~/env/bin/activate
export PYTHONPATH=~/env/lib/python3.7/site-packages:$PYTHONPATH
module load python3/3.7.4
module load openmpi/4.0.5
cd ${WorkDir}PythonMPI/

start=$(date +%s.%N)

mpiexec -np $UseNCPU --map-by numa python3 DynCohesive_ParDataPrepMPI.py $N_MshPrt $dN $ScratchPath

end=$(date +%s.%N)    
runtime=$(python -c "print(${end} - ${start})")
echo "DataPrep walltime is $runtime"
